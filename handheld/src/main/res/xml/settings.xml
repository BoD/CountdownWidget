<?xml version="1.0" encoding="utf-8"?>

<PreferenceScreen xmlns:android="http://schemas.android.com/apk/res/android">
    <Preference
        android:key="PREF_TUTORIAL"
        android:title="@string/preference_tutorial" />

    <Preference
        android:key="PREF_SHARE"
        android:title="@string/preference_share" />

    <org.jraf.android.backport.switchwidget.SwitchPreference
        android:key="PREF_ANDROID_WEAR"
        android:defaultValue="false"
        android:summary="@string/preference_androidWear_summary"
        android:switchTextOff="OFF"
        android:switchTextOn="ON"
        android:title="@string/preference_androidWear_title" />

    <Preference
        android:key="PREF_ABOUT"
        android:title="@string/preference_about" />
</PreferenceScreen>